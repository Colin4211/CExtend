### Build (Generic)
###

build_Generic: 
	-@if [ ! -f "Makefile-Generic" ] ; then CT_Printf -fc "#" "ERROR: Missing file -- #cMakefile-Generic#c" 34 34 ; false ; fi ; 
	-@ $(MAKE) $(MAKEOPTIONS) -C .  -f Makefile-Generic  build

build_Generic_Debug: 
	-@if [ ! -f "Makefile-Generic" ] ; then CT_Printf -fc "#" "ERROR: Missing file -- #cMakefile-Generic#c" 34 34 ; false ; fi ; 
	-@ $(MAKE) $(MAKEOPTIONS) -C .  -f Makefile-Generic  build_Debug

build_Generic_Release: 
	-@if [ ! -f "Makefile-Generic" ] ; then CT_Printf -fc "#" "ERROR: Missing file -- #cMakefile-Generic#c" 34 34 ; false ; fi ; 
	-@ $(MAKE) $(MAKEOPTIONS) -C .  -f Makefile-Generic  build_Release


