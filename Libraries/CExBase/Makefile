### build_auto
###

  ## The output from the dumpmachine command is stored
  ## int this variable to allow us to determine the platform.
GCC_TARGET_ARCH_STRING != gcc -dumpmachine

ifeq  "$(GCC_TARGET_ARCH_STRING)"  "" 
	AUTO_BULD_TARGET_RULE=build_Generic
	AUTO_BULD_TARGET_RULE_DEBUG=build_Generic_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_Generic_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "arm-linux-gnueabihf" 
	AUTO_BULD_TARGET_RULE=build_LinuxARM
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxARM_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxARM_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "i386-linux-gnu" 
	AUTO_BULD_TARGET_RULE=build_LinuxI32
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxI32_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxI32_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "i486-linux-gnu" 
	AUTO_BULD_TARGET_RULE=build_LinuxI32
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxI32_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxI32_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "i586-linux-gnu" 
	AUTO_BULD_TARGET_RULE=build_LinuxI32
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxI32_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxI32_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "i686-linux-gnu" 
	AUTO_BULD_TARGET_RULE=build_LinuxI32
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxI32_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxI32_Release
else ifeq  "$(GCC_TARGET_ARCH_STRING)"  "x86_64-linux-gnu" 
	AUTO_BULD_TARGET_RULE=build_LinuxI64
	AUTO_BULD_TARGET_RULE_DEBUG=build_LinuxI64_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_LinuxI64_Release
else
	AUTO_BULD_TARGET_RULE=build_Generic
	AUTO_BULD_TARGET_RULE_DEBUG=build_Generic_Debug
	AUTO_BULD_TARGET_RULE_RELEASE=build_Generic_Release
endif

build_auto: $(AUTO_BULD_TARGET_RULE) 
	-@

build_auto_Debug: $(AUTO_BULD_TARGET_RULE_DEBUG) 
	-@

build_auto_Release: $(AUTO_BULD_TARGET_RULE_RELEASE) 
	-@


#############################################################################
### Build
###

build: build_auto 
	-@

build_Debug: build_auto_Debug 
	-@

build_Release: build_auto_Release 
	-@

__build_message: 
	-@ CT_Printf -fc "#" "Building == CExtend - Libraries - CExBase" 

__build_message_Debug: 
	-@ CT_Printf -fc "#" "Building == CExtend - Libraries - CExBase - [Debug]" 

__build_message_Release: 
	-@ CT_Printf -fc "#" "Building == CExtend - Libraries - CExBase - [Release]" 

build_all: __build_message build_Generic build_LinuxARM build_LinuxI32 build_LinuxI64 
	-@

build_all_Debug: __build_message_Debug build_Generic_Debug build_LinuxARM_Debug \
		build_LinuxI32_Debug build_LinuxI64_Debug 
	-@

build_all_Release: __build_message_Release build_Generic_Release build_LinuxARM_Release \
		build_LinuxI32_Release build_LinuxI64_Release 
	-@


